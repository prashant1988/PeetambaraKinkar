<h1 class="center">Sub Committee</h1>

<% workbook = Roo::Spreadsheet.open './mpct_about_us.xlsx'%>
<% worksheets = workbook.sheets.second %>

<table class="table_center">
  <%= workbook.sheet(worksheets).each_row_streaming do |rows|  %>
      <tr><% rows.map do|row|%>
            <td><%= row.value %></td>
        <%end%></tr>
  <%end%>
</table>
